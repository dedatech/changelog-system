    {
      "id": "v1.0.0",
      "version": "1.0.1",
      "product": "JetBrains",
      "publishDate": "2026-01-19T10:00:00+08:00",
      "status": "published",
      "title": "Changelog 发布系统首次发布",
      "updates": [
        {
          "id": "1",
          "category": "feature",
          "items": [
            {
              "id": "1-1",
              "text": "核心功能1",
              "children": [
                {
                  "id": "1-1-1",
                  "text": "基于 Next.js 14 的纯静态部署架构，生产环境只需 Nginx"
                },
                {
                  "id": "1-1-2",
                  "text": "支持本地编辑，SSH 自动部署到 Nginx"
                },
                {
                  "id": "1-1-3",
                  "text": "完美响应式设计，支持所有设备"
                },
                {
                  "id": "1-1-4",
                  "text": "简洁的内容编辑方式，无需复杂排版"
                }
              ]
            },
            {
              "id": "1-2",
              "text": "内容管理",
              "children": [
                {
                  "id": "1-2-1",
                  "text": "版本管理（创建、编辑、删除）"
                },
                {
                  "id": "1-2-2",
                  "text": "按产品类型分类（IDE、JetBrains、CLI）"
                },
                {
                  "id": "1-2-3",
                  "text": "支持图片和视频附件（独立展示）"
                }
              ]
            }
          ]
        }
      ]
    },
    {
